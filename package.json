{
  "name": "@supabase/doctest-js",
  "version": "0.2.0",
  "description": "Run doc examples as tests",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "type": "module",
  "scripts": {
    "build": "webpack --mode production",
    "test": "mocha",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint {src,test}/**/*.js webpack.config.js",
    "format": "prettier {src,test}/**/*.js  webpack.config.js  --write",
    "pretty": "prettier --write \"**/*.js\""
  },
  "author": "Supabase",
  "repository": "https://github.com/supabase/doctest-js",
  "license": "MIT",
  "dependencies": {
    "chai": "^4.3.7",
    "jison": "^0.4.18",
    "lex": "^1.7.9"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.3.1",
    "mocha": "^10.1.0",
    "prettier": "^2.8.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  }
}
